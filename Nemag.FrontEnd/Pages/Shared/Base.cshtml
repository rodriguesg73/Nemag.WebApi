<!DOCTYPE html>
<html>

<head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>Nemag Prisma</title>
    <link href="~/font-awesome/css/font-awesome.css?v=@(Nemag.FrontEnd.Global.AppVersion)" rel="stylesheet">
    <link href="~/plugins/jquery-ui/1.12.1/css/jquery-ui.min.css?v=@(Nemag.FrontEnd.Global.AppVersion)" rel="stylesheet">
    <link href="~/css/bootstrap.min.css?v=@(Nemag.FrontEnd.Global.AppVersion)" rel="stylesheet">
    <link href="~/css/animate.css?v=@(Nemag.FrontEnd.Global.AppVersion)" rel="stylesheet">
    <link href="~/css/style.css?v=@(Nemag.FrontEnd.Global.AppVersion)" rel="stylesheet">
    @RenderSection("Styles", required: false)
    <link href="~/css/custom.css?v=@(Nemag.FrontEnd.Global.AppVersion)" rel="stylesheet">
</head>

<body>
    @RenderBody()

    <!-- Mainly scripts -->
    <script src="~/js/template/jquery-3.1.1.min.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
    <script src="~/js/template/popper.min.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
    <script src="~/js/template/bootstrap.min.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>

