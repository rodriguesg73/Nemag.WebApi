    <script src="~/plugins/jquery-ui/1.12.1/js/jquery-ui.min.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
    <script src="~/plugins/slimscroll/1.3.6/js/jquery.slimscroll.min.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
