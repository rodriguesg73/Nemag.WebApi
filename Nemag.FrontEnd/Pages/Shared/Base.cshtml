    <script src="~/plugins/bootbox/5.5.2/js/bootbox.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
    <script src="~/js/template/inspinia.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
    <script src="~/js/pages/Auxiliar.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
    <script src="~/js/pages/_Base/Base.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>
    <script src="~/js/pages/_Helper/Modal.js?v=@(Nemag.FrontEnd.Global.AppVersion)"></script>

    @RenderSection("Scripts", required: false)
    @RenderSection("Modals", required: false)
    @RenderSection("BaseHtml", required: false)
</body>
</html>